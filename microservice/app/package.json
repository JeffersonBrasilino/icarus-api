{
  "name": "icarus-microservice",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:create": "npx typeorm migration:create -n $npm_config_name -o",
    "migration:generate": "npx typeorm migration:generate -n $npm_config_name -o",
    "migration:run": "npx typeorm -c ${APP_DB_CONNECTION_NAME:-default} migration:run",
    "migration:revert": "npx typeorm -c ${APP_DB_CONNECTION_NAME:-default} migration:revert",
    "save-routes-db": "SAVE_ROUTES=true npm run start",
    "generate:entity": "nest g -c @icarus/generator entity",
    "generate:endpoint": "nest g -c @icarus/generator endpoint",
    "test:module": "npx jest src/applications/$npm_config_module"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.1",
    "@nestjs/core": "^8.0.1",
    "@nestjs/cqrs": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.1",
    "@nestjs/swagger": "^5.0.0",
    "@nestjs/typeorm": "^8.0.1",
    "@types/body-parser": "^1.19.1",
    "amqp-connection-manager": "^3.6.0",
    "amqplib": "^0.8.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "helmet": "^4.6.0",
    "nats": "^2.2.0",
    "nodemailer": "^6.6.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.0.24",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-jest": "^26.0.24",
    "ts-loader": "^9.2.3",
    "ts-mockito": "^2.6.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^@applications/(.*)$": "<rootDir>/applications/$1",
      "^@infrastructure/(.*)$": "<rootDir>/../dist/infrastructure/$1"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
